{
  "name": "nsv-action",
  "version": "1.0.0",
  "description": "Semantic versioning without any config",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/purpleclay/nsv-action.git"
  },
  "keywords": [
    "github",
    "actions",
    "semver",
    "release",
    "golang",
    "go"
  ],
  "author": "purpleclay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/purpleclay/nsv-action/issues"
  },
  "homepage": "https://github.com/purpleclay/nsv-action",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "tmp": "^0.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
